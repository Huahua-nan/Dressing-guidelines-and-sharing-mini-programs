page,.page{
  min-height: 100%;
  background-color: #f8f8f8;
}
.list{
  display: flex;
  flex-wrap: wrap;
}
.item{
  width: 50%;
  box-sizing: border-box;
  padding: 10rpx;
}
.item .content{
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
  width: 100%;
}
.item .item-img{
  width: 100%;
}
.item .item-img .img{
  width: 100%;
  height: 345rpx;
  vertical-align: middle;
  margin-bottom: 10rpx;
}
.item .name{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 11pt;
  font-weight: bold;
  color: #333;
  text-align: center;
}
.leftList,.rightList{
  float: left;
  width: 50%;
}
.leftList .item,.rightList .item{
  width: 100%;
}
.clear-float{
  clear: both;
}